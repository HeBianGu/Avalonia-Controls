<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:h="https://github.com/HeBianGu"
                    xmlns:local="clr-namespace:Avalonia.Theme"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <h:AssetBitmap x:Key="{StringResourceKey ResourceId=S.BackgroundImage.BackgroundImage, TypeInTargetAssembly={x:Type local:BackgroundImageKeys}}">
        avares://Avalonia.Extensions.BackgroundImage/Assets/Arrow.png
    </h:AssetBitmap>
    <system:Double x:Key="{StringResourceKey ResourceId=S.BackgroundImage.Opacity, TypeInTargetAssembly={x:Type local:BackgroundImageKeys}}">0.2</system:Double>
    <Stretch x:Key="{StringResourceKey ResourceId=S.BackgroundImage.Stretch, TypeInTargetAssembly={x:Type local:BackgroundImageKeys}}">UniformToFill</Stretch>
</ResourceDictionary>
