              <local:Function Name="仅聊天的朋友"/>
              <local:Function Name="群聊"/>
              <local:Function Name="标签"/>
              <local:Function Name="公众号"/>
            </ListBox>
            <TextBlock Margin="5"
                       DockPanel.Dock="Top"
                       FontSize="10"
