                            <m:Function Name="朋友圈" />
                            <m:Function Name="卡包" />
                            <m:Function Name="表情" />
                        </ListBox>

                        <ListBox Classes="goto">
                            <m:SettingFunction Name="设置" />
                        </ListBox>

                    </StackPanel>
                </ScrollViewer>
            </DockPanel>
        </DataTemplate>
