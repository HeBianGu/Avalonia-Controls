                    <Interaction.Behaviors>
                      <EventTriggerBehavior EventName="PointerPressed">
                        <InvokeCommandAction Command="{Binding ShowCommand}" />
                      </EventTriggerBehavior>
                    </Interaction.Behaviors>
                    <Border Width="40"
                            Height="40"
                            Margin="0,0,10,0"
                            ClipToBounds="True"
                            CornerRadius="5">
                      <Image Source="/Assets/1.jpg" Stretch="UniformToFill" />
                    </Border>
