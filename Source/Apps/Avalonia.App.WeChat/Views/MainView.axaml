<UserControl x:Class="Avalonia.App.WeChat.Views.MainView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:h="https://github.com/HeBianGu"
             xmlns:local="clr-namespace:Avalonia.App.WeChat.Views"
             xmlns:m="clr-namespace:Avalonia.App.WeChat.Models"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             xmlns:vm="clr-namespace:Avalonia.App.WeChat.ViewModels"
             d:DesignHeight="800"
             d:DesignWidth="400"
             x:DataType="vm:MainViewModel"
             Background="{DynamicResource {x:Static h:BrushKeys.Background}}"
             Foreground="{DynamicResource {x:Static h:BrushKeys.Foreground}}"
             mc:Ignorable="d">
    <Design.DataContext>
        <!--
            This only sets the DataContext for the previewer in an IDE,
            to set the actual DataContext for runtime, set the DataContext property in code (look at App.axaml.cs)
        -->
        <vm:MainViewModel />
    </Design.DataContext>
    <UserControl.Resources>
        <ControlTheme x:Key="S.ListBoxItem.Fuction"
                      BasedOn="{StaticResource {x:Type ListBoxItem}}"
                      TargetType="ListBoxItem">
            <Style Selector="^:selected">
                <Style Selector="^ /template/ ContentPresenter#PART_ContentPresenter">
                    <Setter Property="Background" Value="Transparent" />
                    <Setter Property="Foreground" Value="{DynamicResource {x:Static h:BrushKeys.Foreground}}" />
                </Style>
                <Style Selector="^:pointerover /template/ ContentPresenter#PART_ContentPresenter">
                    <Setter Property="Background" Value="{DynamicResource {x:Static h:BrushKeys.MouseOver}}" />
                </Style>
            </Style>
        </ControlTheme>

        <!--<ControlTheme x:Key="S.ListBox.Fuction" TargetType="ListBox" BasedOn="{StaticResource {x:Type ListBox}}">
      <Setter Property="Background" Value="{DynamicResource {x:Static h:BrushKeys.Dark0_1}}"/>
      <Style Selector="ListBox.message">
        <Setter Property="Background" Value="{DynamicResource {x:Static h:BrushKeys.Dark0_1}}"/>
        <Setter Property="ItemContainerTheme">
          <Setter.Value>
            <ControlTheme BasedOn="{StaticResource S.ListBoxItem.Fuction}" TargetType="ListBoxItem">
              <Setter Property="Height" Value="60" />
            </ControlTheme>
          </Setter.Value>
        </Setter>
      </Style>
    </ControlTheme>-->
    </UserControl.Resources>
    <UserControl.Styles>
        <Style Selector="ListBox.message">
            <Setter Property="Background" Value="{DynamicResource {x:Static h:BrushKeys.Dark0_1}}" />
            <Setter Property="DockPanel.Dock" Value="Top" />
            <Setter Property="ItemContainerTheme">
                <Setter.Value>
                    <ControlTheme BasedOn="{StaticResource S.ListBoxItem.Fuction}" TargetType="ListBoxItem">
                        <Setter Property="Height" Value="60" />
                    </ControlTheme>
                </Setter.Value>
            </Setter>
        </Style>

        <Style Selector="ListBox.function">
            <Setter Property="Background" Value="{DynamicResource {x:Static h:BrushKeys.Dark0_1}}" />
            <Setter Property="DockPanel.Dock" Value="Top" />
            <Setter Property="ItemContainerTheme">
                <Setter.Value>
                    <ControlTheme BasedOn="{StaticResource S.ListBoxItem.Fuction}" TargetType="ListBoxItem">
                        <Setter Property="Height" Value="50" />
                    </ControlTheme>
                </Setter.Value>
            </Setter>
            <Setter Property="ItemTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <DockPanel Background="Transparent">
                            <Interaction.Behaviors>
                                <EventTriggerBehavior EventName="PointerPressed">
                                    <InvokeCommandAction Command="{Binding ShowCommand}" />
                                </EventTriggerBehavior>
                            </Interaction.Behaviors>
                            <Border Width="35"
                                    Height="35"
                                    Margin="0,0,10,0"
                                    ClipToBounds="True"
                                    CornerRadius="5">
                                <Image Source="/Assets/1.jpg" Stretch="UniformToFill" />
                            </Border>
                            <TextBlock VerticalAlignment="Center" Text="{Binding}" />
                        </DockPanel>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style Selector="ListBox.adress">
            <Setter Property="Background" Value="{DynamicResource {x:Static h:BrushKeys.Dark0_1}}" />
            <Setter Property="DockPanel.Dock" Value="Top" />
            <Setter Property="ItemContainerTheme">
                <Setter.Value>
                    <ControlTheme BasedOn="{StaticResource S.ListBoxItem.Fuction}" TargetType="ListBoxItem">
                        <Setter Property="Height" Value="50" />
                    </ControlTheme>
                </Setter.Value>
            </Setter>
            <Setter Property="ItemTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <DockPanel Background="Transparent">
                            <Interaction.Behaviors>
                                <EventTriggerBehavior EventName="PointerPressed">
                                    <InvokeCommandAction Command="{Binding ShowCommand}" />
                                </EventTriggerBehavior>
                            </Interaction.Behaviors>
                            <Border Width="35"
                                    Height="35"
                                    Margin="0,0,10,0"
                                    ClipToBounds="True"
                                    CornerRadius="5">
                                <Image Source="/Assets/1.jpg" Stretch="UniformToFill" />
                            </Border>
                            <TextBlock VerticalAlignment="Center" Text="{Binding Name}" />
                        </DockPanel>
                    </DataTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style Selector="ListBox.goto">
            <Setter Property="Background" Value="{DynamicResource {x:Static h:BrushKeys.Dark0_1}}" />
            <Setter Property="DockPanel.Dock" Value="Top" />
            <Setter Property="Margin" Value="0 2" />
            <Setter Property="ItemContainerTheme">
                <Setter.Value>
                    <ControlTheme BasedOn="{StaticResource S.ListBoxItem.Fuction}" TargetType="ListBoxItem">
                        <Setter Property="Height" Value="40" />
                    </ControlTheme>
                </Setter.Value>
            </Setter>
        </Style>
    </UserControl.Styles>
    <UserControl.DataTemplates>
        <DataTemplate DataType="{x:Type m:Address}">
            <DockPanel>
                <Border Width="40"
                        Height="40"
                        Margin="0,0,10,0"
                        ClipToBounds="True"
                        CornerRadius="5">
                    <Image Source="/Assets/1.jpg" Stretch="UniformToFill" />
                </Border>
                <TextBlock VerticalAlignment="Center"
                           FontSize="{DynamicResource {x:Static h:FontSizeKeys.Header5}}"
                           Foreground="{DynamicResource {x:Static h:BrushKeys.Foreground}}"
                           Text="{Binding Name}" />
            </DockPanel>
        </DataTemplate>
        <DataTemplate DataType="{x:Type m:Message}">
            <DockPanel Background="Transparent">
                <Interaction.Behaviors>
                    <EventTriggerBehavior EventName="Tapped">
                        <InvokeCommandAction Command="{Binding ShowCommand}" />
                    </EventTriggerBehavior>
                </Interaction.Behaviors>
                <Border Width="40"
                        Height="40"
                        Margin="0,0,10,0"
                        ClipToBounds="True"
                        CornerRadius="5">
                    <Image Source="/Assets/1.jpg" Stretch="UniformToFill" />
                </Border>
                <UniformGrid Columns="1">
                    <Grid>
                        <TextBlock VerticalAlignment="Center"
                                   FontSize="{DynamicResource {x:Static h:FontSizeKeys.Header5}}"
                                   Foreground="{DynamicResource {x:Static h:BrushKeys.Foreground}}"
                                   Text="{Binding Name}" />
                        <TextBlock HorizontalAlignment="Right"
                                   VerticalAlignment="Center"
                                   FontSize="{DynamicResource {x:Static h:FontSizeKeys.Header7}}"
                                   Foreground="{DynamicResource {x:Static h:BrushKeys.ForegroundAssist}}"
                                   Text="{Binding Time, Converter={m:TimeValueConverter}}" />
                    </Grid>
                    <TextBlock VerticalAlignment="Center"
                               FontSize="{DynamicResource {x:Static h:FontSizeKeys.Header6}}"
                               Foreground="{DynamicResource {x:Static h:BrushKeys.ForegroundAssist}}"
                               Text="{Binding Address}" />
                </UniformGrid>
            </DockPanel>
        </DataTemplate>
