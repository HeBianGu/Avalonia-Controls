    <DataTemplate DataType="local:MyItem">
      <DockPanel>
        <Grid Background="{DynamicResource {x:Static h:BrushKeys.Dark0_1}}" DockPanel.Dock="Top">
          <DockPanel Margin="10,20,10,10">
            <Border Width="60"
                    Height="60"
                    Margin="0,0,10,0"
                    ClipToBounds="True"
                    CornerRadius="5">
              <Image Source="/Assets/logo.ico" Stretch="UniformToFill" />

            </Border>
            <UniformGrid Columns="1">
              <TextBlock VerticalAlignment="Center"
                         FontSize="{DynamicResource {x:Static h:FontSizeKeys.Header3}}"
                         FontWeight="Bold"
                         Foreground="{DynamicResource {x:Static h:BrushKeys.Foreground}}"
                         Text="HeBianGu" />
              <TextBlock Margin="0,8"
                         VerticalAlignment="Center"
                         FontSize="12"
                         Text="微信号：908293466" />
              <DockPanel>
                <Button Height="NaN"
                        Padding="5,3"
                        FontSize="{DynamicResource {x:Static h:FontSizeKeys.Header7}}"
                        BorderBrush="{DynamicResource {x:Static h:BrushKeys.BorderBrush}}"
                        Content="+ 状态"
                        CornerRadius="15" />
                <Button Height="NaN"
                        Padding="5,3"
                        FontSize="{DynamicResource {x:Static h:FontSizeKeys.Header7}}"
                        BorderBrush="{DynamicResource {x:Static h:BrushKeys.BorderBrush}}"
                        Content="3个朋友"
                        CornerRadius="15" />
              </DockPanel>
            </UniformGrid>
          </DockPanel>
        </Grid>
        <ScrollViewer>
          <StackPanel>
            <ListBox Classes="goto">
              <local:Function Name="服务"/>
            </ListBox>

            <ListBox Classes="goto">
              <local:Function Name="收藏"/>
              <local:Function Name="朋友圈"/>
              <local:Function Name="卡包"/>
              <local:Function Name="表情"/>
            </ListBox>

            <ListBox Classes="goto">
              <local:Function Name="朋友圈"/>
            </ListBox>

            <UniformGrid Columns="1">
              <Button
          Command="{h:ShowBackAdornerDialogCommand}"
          Content="ShowBackAdornerDialogCommand">
              </Button>
              <Button Command="{ShowCommand Presnter={h:GetStudent}}" Content="ShowCommand" />
              <Button Command="{ShowStringCommand}" Content="ShowStringCommand" />
              <Button Command="{ShowPercentCommand}" Content="ShowPercentCommand" />
              <Button Command="{ShowWaitCommand}" Content="ShowWaitCommand" />
              <Button Command="{ShowMessageCommand}" Content="ShowMessageCommand" />
              <Button
                      Command="{h:ShowSettingCommand}"
                      Content="ShowSettingCommand"
                      DockPanel.Dock="Right">
              </Button>
              <Button
                      Command="{h:ShowThemeSettingCommand}"
                      Content="ShowThemeSettingCommand"
                      DockPanel.Dock="Right"> </Button>

              <Button
                  Command="{h:ShowAboutCommand}"
                  Content="ShowAboutCommand">
              </Button>

              <Button Command="{ShowEditCommand Value={h:GetStudent}}" Content="ShowEditCommand" />
              <Button Command="{ShowViewCommand Value={h:GetStudent}}" Content="ShowViewCommand" />

              <Button Command="{h:ShowInfoSnackMessageCommand Message=我是Info消息}" Content="ShowInfo" />
              <Button Command="{h:ShowErrorSnackMessageCommand Message=我是Error消息}" Content="ShowError" />
              <Button Command="{h:ShowSuccessSnackMessageCommand Message=我是Success消息}" Content="ShowSuccess" />
              <Button Command="{h:ShowWarnSnackMessageCommand Message=我是Warn消息}" Content="ShowWarn" />
              <Button Command="{h:ShowFatalSnackMessageCommand Message=我是Fatal消息}" Content="ShowFatal" />
              <Button Command="{h:ShowProgressSnackMessageCommand Message=我是Progress消息}" Content="ShowProgress" />
              <Button Command="{h:ShowStringSnackMessageCommand Message=我是String消息}" Content="ShowString" />
              <Button Command="{h:ShowDialogSnackMessageCommand Message=我是Dialog消息}" Content="ShowDialog" />

            </UniformGrid>
          </StackPanel>
        </ScrollViewer>
      </DockPanel>
    </DataTemplate>
