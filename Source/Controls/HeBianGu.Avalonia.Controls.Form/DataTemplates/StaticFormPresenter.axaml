<DataTemplate xmlns="https://github.com/avaloniaui"
              xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
              xmlns:local="clr-namespace:HeBianGu.Avalonia.Controls.Form"
              DataType="{x:Type local:StaticFormPresenter}">
    <local:Form Margin="10,6"
                SelectObject="{Binding Value}"
                UseAsync="False"
                UsePropertyView="{Binding UsePropertyView}" />
</DataTemplate>