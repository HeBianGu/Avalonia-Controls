<DataTemplate xmlns="https://github.com/avaloniaui"
              xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
              xmlns:local="clr-namespace:HeBianGu.AvaloniaUI.Form"
              DataType="{x:Type local:StaticFormPresenter}">
    <local:Form Margin="{DynamicResource {x:Static LayoutKeys.Padding}}"
                SelectObject="{Binding Value}"
                UseAsync="False"
                UsePropertyView="{Binding UsePropertyView}" />
</DataTemplate>